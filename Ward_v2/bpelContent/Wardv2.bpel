                    <![CDATA[concat('Patient ID: ', bpel:getVariableData('PatientDataPLResponse3', 'RadiologyReport','/PatientID'), "\n",			 'Radiology examination ID: ', bpel:getVariableData('PatientDataPLResponse3', 'RadiologyReport','/RadiologyOrderID'), '\n',			 'Laboratory examination ID: ', bpel:getVariableData('PatientDataPLResponse2', 'LabReport','/LabOrderID'), '\n',			 'Radiology result: ', bpel:getVariableData('PatientDataPLResponse3', 'RadiologyReport','/ReportText'), '\n',			 'Lab result: ', bpel:getVariableData('PatientDataPLResponse2', 'LabReport','/LabValues'), '\n',			 'Based on the aforementioned data, we conclude that the patient : ', 			 			bpel:getVariableData('clientRequest', 'validateReport','/tns:Validation'), '\n')]]>
