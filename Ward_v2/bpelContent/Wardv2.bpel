        <bpel:assign validate="no" name="Assign-dischargeletter">
            
            
            <bpel:copy>
                <bpel:from><bpel:literal><tns:ValidateReportResponse xmlns:tns="http://www.PAHospital.org/ward" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
